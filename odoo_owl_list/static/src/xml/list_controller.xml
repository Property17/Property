<templates id="template" xml:space="preserve">
    <t t-name="odoo_owl_list.ListView" t-inherit="web.ListView" t-inherit-mode="extension">
        <xpath expr="//t[@t-set-slot='control-panel-additional-actions']/CogMenu" position="after">
            <button t-if="props.resModel == 'account.analytic.account' and showReminderButton"
                class="o_btn_hello btn btn-danger me-1"
                data-hotkey="r"
                t-on-click="openExpirationReminder">
                <i class="fa fa-exclamation-triangle me-1"></i> Tenancy Expiration Reminder
            </button>
        </xpath>
    </t>
</templates>